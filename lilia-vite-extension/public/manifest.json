{
    "manifest_version": 3,
    "version": "1.0.1",
    "name": "Lilia Vite extension",
    "icons": {"128": "icon128.png"},
    "action": {
        "default_popup": "index.html"
    },
    "permissions": ["contextMenus", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}